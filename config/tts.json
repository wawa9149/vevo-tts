{
  "base_config": "config/base.json",  
  "supported_model_type": [
    "Fastspeech2",
    "VITS",
    "VALLE",
    "NaturalSpeech2"
  ],
  "task_type": "tts",
  "preprocess": {
    "language": "ko", //  g2pK supports Korean language
    // linguistic features
    "extract_phone": true,
    "phone_extractor": "korean_ipa", // "espeak, pypinyin, pypinyin_initials_finals, lexicon, g2pk, korean_ipa"
    "lexicon_path": "./text/lexicon/librispeech-lexicon.txt",
    // Directory names of processed data or extracted features
    "phone_dir": "phones",
    "use_phone": true,
    "add_blank": true
  },
  "model": {
      "text_token_num": 512
  }

}
